{
  "$schema": "https://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "order_item_id",
    "rating",
    "comment",
    "temporary_media_ids",
    "old_file_ids",
    "is_anonymous"
  ],
  "properties": {
    "order_item_id": {
      "type": "null"
    },
    "rating": {
      "type": "integer",
      "default": 0
    },
    "comment": {
      "type": "string"
    },
    "temporary_media_ids": {
      "type": "array",
      "items": {},
      "default": []
    },
    "old_file_ids": {
      "type": "array",
      "items": {},
      "default": []
    },
    "is_anonymous": {
      "type": "boolean",
      "default": false
    }
  }
}
