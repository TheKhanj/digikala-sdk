      "type": "string"
    },
    "customer": {
      "$ref": "#/definitions/Customer"
    },
    "proxies": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/definitions/HttpProxy"
      }
    }
  },
  "definitions": {
    "Customer": {
      "type": "object",
      "additionalProperties": false,
