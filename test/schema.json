        "username",
        "password"
      ],
      "properties": {
        "proxy": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      }
    },
    "HttpProxy": {
      "type": "object",
      "additionalProperties": false,
      "required": [
